page {
  background: #EDECE8;
}
.item {
  background: #FFFFFF;
  width: 350rpx;
  height: 200rpx;
  float: left;
  margin-left: 25rpx;
  margin-bottom: 20rpx;
  position: relative;
}
.item image {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-left: -50rpx;
  margin-top: -50rpx;
}
